# Template only. Environment variables are not expanded by ssh_config.
# Use scripts/generate_ssh_config.sh to produce config/ssh_config.
Include ~/.ssh/config
Host stx-aio-0
  HostName ${STX_AIO_HOSTNAME}
  User ${STX_AIO_USER}
  Port ${STX_AIO_PORT}
  IdentityFile ${STX_AIO_IDENTITY_FILE}
  IdentitiesOnly yes
  ServerAliveInterval 60
  ServerAliveCountMax 3
  StrictHostKeyChecking accept-new

# Notes:
# - This file documents which variables are expected for CI.
# - Do not rely on this file at runtime; always generate config/ssh_config or use ~/.ssh/config.
