{
  "name": "@agentdb/plugin-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for SQLite Vector learning plugins",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run unit/",
    "test:integration": "vitest run integration/",
    "test:cli": "vitest run cli/",
    "test:templates": "vitest run templates/",
    "test:benchmarks": "vitest run benchmarks/",
    "test:verbose": "vitest run --reporter=verbose",
    "test:single": "vitest run --reporter=verbose --no-coverage"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "sqlite",
    "vector",
    "machine-learning",
    "reinforcement-learning",
    "testing",
    "vitest"
  ]
}
